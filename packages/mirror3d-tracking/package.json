{
  "name": "@mirrorx/mirror3d-tracking",
  "version": "1.0.0",
  "description": "MediaPipe pose and face tracking wrappers for 3D Virtual Mirror",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@mediapipe/tasks-vision": ">=0.10.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "@types/node": "^20.10.0"
  },
  "keywords": ["mediapipe", "pose", "tracking", "3d-mirror"],
  "license": "MIT"
}
